{
  "name": "elbucho/database",
  "description": "Utility wrapper for the PDO subsystem",
  "keywords": ["pdo","database"],
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Hinkson",
      "email": "mjhinkson@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "doctrine/inflector": "^1.0",
    "pds/skeleton": "^1.0",
    "elbucho/config": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "phpunit/php-code-coverage": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Elbucho\\Database\\": "src/"
    }
  },
  "autoload-dev": {
    "Elbucho\\Database\\Tests\\": "tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
